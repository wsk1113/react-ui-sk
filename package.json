{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "gpush": "git add . && git commit -m temp && git push -u origin dev-rollup",
    "build:lib": "rimraf dist && npx rollup -c rollup/rollup.config.js",
    "publish:lib": "cd dist && npm publish --access=public",
    "release": "npm run build:lib && npm run publish:lib"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "axios": "^1.4.0",
    "pusher-js": "^8.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "rollup-plugin-babel": "^4.4.0",
    "sass": "^1.64.1",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.6",
    "@types/spark-md5": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "rollup": "^2.79.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svg": "^2.0.0",
    "sass": "^1.64.1",
    "tslib": "^2.6.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "xlsx": "^0.18.5"
  }
}
